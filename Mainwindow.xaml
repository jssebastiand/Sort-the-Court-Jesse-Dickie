<Window x:Class="Sort_The_Court_Remake_Culminating.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sort_The_Court_Remake_Culminating"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Canvas x:Name="canvas">
        <Image Source="https://vignette.wikia.nocookie.net/sort-the-court/images/2/24/The_King.PNG/revision/latest?cb=20171108044936" Height="250" Width="250" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label Content=""/>
        <Rectangle x:Name="tempRect" Fill="Blue" Width="125" Height="125" Canvas.Left="800" Canvas.Top="125"/>
        <!--<Image Source="georgie.jpeg" Height="125" Width="125"/>-->
        <Button Content="YES" Background="LimeGreen" x:Name="btnYes" Click="btnYes_Click" Width="100" Canvas.Left="250" Canvas.Top="275"/>
        <Button Content="NO" Background="IndianRed" x:Name="btnNo" Click="btnNo_Click" Width="100" VerticalAlignment="Bottom" Canvas.Left="400" Canvas.Top="275"/>

    </Canvas>
</Window>
